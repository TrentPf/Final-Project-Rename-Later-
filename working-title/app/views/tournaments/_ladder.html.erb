<article id="ladder">
	<ul class="round round-1">
		<li class="spacer">&nbsp;</li>
			<% if !starting_seeds %>
				<li class="team team-a">&nbsp;</li>
				<li class="team team-spacer">&nbsp;</li>
				<li class="team team-b">&nbsp;</li>

				<li class="spacer">&nbsp;</li>
				
				<li class="team team-a">&nbsp;</li>
				<li class="team team-spacer">&nbsp;</li>
				<li class="team team-b">&nbsp;</li>

				<li class="spacer">&nbsp;</li>
				
				<li class="team team-a ">&nbsp;</li>
				<li class="team team-spacer">&nbsp;</li>
				<li class="team team-b">&nbsp;</li>

				<li class="spacer">&nbsp;</li>
				
				<li class="team team-a">&nbsp;</li>
				<li class="team team-spacer">&nbsp;</li>
				<li class="team team-b">&nbsp;</li>
			<% elsif starting_seeds.length == 8 %>
				<li class="team team-a"><%= starting_seeds[0].username %></li>
				<li class="team team-spacer">&nbsp;</li>
				<li class="team team-b"><%= starting_seeds[1].username %></li>

				<li class="spacer">&nbsp;</li>
				
				<li class="team team-a"><%= starting_seeds[2].username %></li>
				<li class="team team-spacer">&nbsp;</li>
				<li class="team team-b"><%= starting_seeds[3].username %></li>

				<li class="spacer">&nbsp;</li>
				
				<li class="team team-a "><%= starting_seeds[4].username %></li>
				<li class="team team-spacer">&nbsp;</li>
				<li class="team team-b"><%= starting_seeds[5].username %></li>

				<li class="spacer">&nbsp;</li>
				
				<li class="team team-a"><%= starting_seeds[6].username %></li>
				<li class="team team-spacer">&nbsp;</li>
				<li class="team team-b"><%= starting_seeds[7].username %></li>
			<% end %>
			<li class="spacer">&nbsp;</li>
		</ul>
		<ul class="round round-2"> 
			<li class="spacer">&nbsp;</li>
			
			<li class="team team-a"><%= matches.match1 %></li>
			<li class="team team-spacer">&nbsp;</li>
			<li class="team team-b"><%= matches.match2 %></li>

			<li class="spacer">&nbsp;</li>
			
			<li class="team team-a"><%= matches.match3 %></li>
			<li class="team team-spacer">&nbsp;</li>
			<li class="team team-b"><%= matches.match4 %></li>


		<li class="spacer">&nbsp;</li>
	</ul>
	<ul class="round round-3">
		<li class="spacer">&nbsp;</li>
		
		<li class="team team-a"><%= matches.match5 %></li>
		<li class="team team-spacer">&nbsp;</li>
		<li class="team team-b"><%= matches.match6 %></li>

		<li class="spacer">&nbsp;</li>
	</ul>
  <ul class="round winner">
		<li class="spacer">&nbsp;</li>
		<i class="fas fa-crown"></i>
		<li class="team winning-team"><%= matches.match7 %></li>

		<li class="spacer">&nbsp;</li>
	</ul>
</article>